{
    "name": "faina-portfolio",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "prisma": "npx prisma",
        "db:migrate": "prisma migrate dev",
        "db:push": "prisma db push",
        "db:reset": "prisma migrate reset --force",
        "db:generate": "prisma generate",
        "db:studio": "prisma studio",
        "db:deploy": "prisma migrate deploy",
        "db:update": "prisma generate && prisma db push && prisma migrate dev",
        "db:clear-paintings": "prisma db execute --stdin < ./prisma/scripts/clear-paintings.sql",
        "db:clear-categories": "prisma db execute --stdin < ./prisma/scripts/clear-categories.sql",
        "db:clear": "npm run db:clear-paintings && npm run db:clear-categories"
    },
    "prisma": {
        "schema": "prisma/schema.prisma"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.750.0",
        "@prisma/client": "^6.4.0",
        "bcrypt": "^5.1.1",
        "formidable": "^3.5.2",
        "fs-extra": "^11.3.0",
        "jose": "^5.10.0",
        "next": "latest",
        "prisma": "^6.4.0",
        "react": "latest",
        "react-dom": "latest",
        "react-toastify": "^11.0.3",
        "sass": "^1.85.0",
        "sharp": "^0.33.5"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/formidable": "^3.4.5",
        "@types/fs-extra": "^11.0.4",
        "@types/node": "latest",
        "@types/react": "latest",
        "@types/react-dom": "latest",
        "typescript": "5.7.3"
    }
}
